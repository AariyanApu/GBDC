// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
model Post {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  title  String
  slug   String    @unique
  desc   String
  imgUrl String
  views Int @default(0)
  createdAt DateTime? @default(now())
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String
}

model Activity {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  title String
  imgUrl String
  tag  String
}

model another {
 id    String @id @default(auto()) @map("_id") @db.ObjectId
 title String
 imgUrl String 
}
model Notice {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  title  String
  slug   String    @unique
  desc   String
  views Int @default(0)
  createdAt DateTime? @default(now())
}